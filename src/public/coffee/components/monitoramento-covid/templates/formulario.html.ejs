<style type='text/css'>
.form-control-wrapper {
    float: right!important;
}
input{
    max-width:80px!important;
    text-align: center;
    background-color: #eee!important;
    border: 2px solid #eee!important;
    border-radius: 4px!important;
}
.local-heading{
    background-color: #CCE1EF!important;
    font-weight: 600;
}

.local-heading-group{
    background-color: #ebf6fc!important;
    font-weight: 600;
}
.panel-heading h3{
    color:#2C5A7A!important;
    font-weight: 600;
}
.panel-heading {
    color:#2C5A7A!important;
    font-weight: 600;
}
li {
  list-style-type: none;
}
.btn-navegar{
    background-color:#2c5a7a!important;
}
